{
  "manifest_version": 3,
  "name": "SL Script",
  "version": "1.0",
  "description": "По всім питанням: Назар - https://t.me/usdcethbtc",
  "author": "Назар",
  "homepage_url": "https://t.me/usdcethbtc",
  "permissions": ["storage", "activeTab", "scripting", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}